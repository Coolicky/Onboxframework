<mvc:MvcComponentBase x:Class="$rootnamespace$.$safeitemname$"
					DataContext="{Binding RelativeSource={RelativeSource Self}}"
					xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
					xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
					xmlns:local="clr-namespace:$rootnamespace$"
					xmlns:mvc="clr-namespace:Onbox.Mvc.VDev;assembly=Onbox.Mvc.VDev"
					xmlns:rmvc="clr-namespace:Onbox.Mvc.Revit.VDev;assembly=Onbox.Mvc.Revit.VDev"
					mc:Ignorable="d"
					d:DesignHeight="450"
					d:DesignWidth="500">

	<!--It is necessary to name all Navigator Components-->
	<!--You may have multiple Navigators as long as you name them differently-->
	<mvc:NavigatorComponent x:Name="Navigator"/>

</mvc:MvcComponentBase>